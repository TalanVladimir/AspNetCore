@model IEnumerable<AspNetCore.Models.Member>

@{
    ViewData["Title"] = "Database";
}

<h1>@ViewData["Title"]</h1>

<div id="modDialog" class="modal fade">
    <div id="dialogContent" class="modal-dialog"></div>
</div>

<form method="post" enctype="multipart/form-data" asp-controller="Members" asp-action="DatabaseUpload">
    <table class="table">
        <thead>
            <tr>
                <th scope="col"><input class="form-control" name="files" type="file" /></th>
                <th scope="col"><input class="btn btn-success" type="submit" value="Upload File" /></th>
                <th scope="col"><a class="btn btn-primary event" asp-action="DatabaseDownload"><i class="fa fa-trash"></i>Download CSV</a></th>
                <th scope="col"><a class="btn btn-danger event" asp-action="DatabaseClear"><i class="fa fa-trash"></i>Drop Database</a></th>
            </tr>
        </thead>
    </table>
</form>
